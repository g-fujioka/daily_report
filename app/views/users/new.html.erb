<% provide(:title, '新規登録') %>
<% provide(:button_text, '登録する') %>
<h1>新規登録</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user, url: users_path) do |f| %>
        <%= render 'form', {f: f} %>
    <% end %>
  </div>
</div>

<table border="1">
  <tr><th>部署ID</th><th>部署名</th></tr>
  <% @departments.each do |department| %>
      <tr>
        <td><%= department.id %></td><td><%= department.department_name %></td>
      </tr>
  <% end %>
</table>